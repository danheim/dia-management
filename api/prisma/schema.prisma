generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Projects {
  id           Int       @id @default(autoincrement())
  title        String
  description  String
  creationDate DateTime  @default(now())
  releaseDate  DateTime  @default(now())
  startDate    DateTime?
}

model Users {
  id       Int    @id @default(autoincrement())
  username String
  password String
}
